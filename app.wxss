@import "./style/animate.wxss";
@import "./style/weui.wxss";

page {
    background: #eee;
    height: 100%;
}

/* 清除浮动 */

.clear-fix:after {
    content: "\0020";
    display: block;
    height: 0;
    clear: both;
}

.clear-fix {
    zoom: 1;
}

.footer {
    color: #888;
    font-size: 12px;
    text-align: center;
    background: #eee;
    padding: 20px 0;
}

.footer .logoWrap {
    width: 88px;
    height: 20px;
    margin: 10px auto;
}

.footer .logoWrap image {
    width: 88px;
    height: 20px;
}

.footer .gap {
    padding: 0 4px;
}

ad {
    transform: scale(0.95) !important;
    margin-bottom: 10px
}

